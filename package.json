{
  "name": "tsed-starter-kit",
  "version": "1.0.0",
  "description": "webpack based starter kit for @tsed",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "npm run tsc",
    "tslint": "./node_modules/.bin/tslint ./*.ts ./lib/*.ts",
    "test": "npm run clean && npm run tsc && npm run tslint && npm run nyc",
    "nyc": "NODE_PATH=./src NODE_ENV=test ./node_modules/.bin/nyc --reporter=html --reporter=text _mocha \"{src,test}/**/*.spec.js\" --recursive",
    "coveralls:travis": "./node_modules/.bin/nyc report --reporter=text-lcov | coveralls",
    "tsc": "./node_modules/.bin/webpack-cli --config webpack.prod.js",
    "tsc:w": "NODE_PATH=./src ./node_modules/.bin/tsc --project tsconfig.json -w",
    "seed": "node ./seeders/index.js",
    "add-migrations": "NODE_PATH=./src ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "update-database": "NODE_PATH=./src ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run",
    "rollback-database": "NODE_PATH=./src ./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert",
    "start:dev": "NODE_ENV=development node_modules/.bin/webpack-cli --config webpack.dev.js",
    "start": "NODE_ENV=production node ./main.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnjerrico/tsed-starter-kit.git"
  },
  "keywords": [
    "@tsed",
    "typescript",
    "decorators",
    "express"
  ],
  "author": "romakita, johnjerrico",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnjerrico/tsed-starter-kit/issues"
  },
  "homepage": "https://github.com/johnjerrico/tsed-starter-kit#readme",
  "dependencies": {
    "@tsed/common": "^4.30.1",
    "@tsed/core": "^4.30.1",
    "@tsed/multipartfiles": "^4.30.1",
    "aws-sdk": "^2.282.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "config": "^1.30.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "method-override": "^2.3.10",
    "multer": "^1.3.1",
    "pg": "^7.4.3",
    "ts-log-debug": "^4.0.4",
    "typeorm": "0.2.7",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@tsed/testing": "^4.26.0",
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.16.0",
    "@types/multer": "^1.3.7",
    "@types/node": "^8.0.29",
    "@types/request-promise": "^4.1.41",
    "@types/sinon": "^5.0.1",
    "@types/sinon-chai": "^3.2.0",
    "@types/supertest": "^2.0.4",
    "@types/swagger-schema-official": "^2.0.10",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "gaze": "^1.1.3",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.0",
    "sinon-chai": "^3.2.0",
    "supertest": "^3.1.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  }
}
